<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean class="com.java.collection.injection.Employee" name="emp1">
		<property name="name" value="Meraj ALam" />
		<property name="phones">
			<list>
				<value>7833993939</value>
				<value>8787784545</value>
				<value>7833989890</value>

			</list>

		</property>

		<property name="address">
			<set>
				<value>Basti</value>
				<value>Lucknow</value>
				<value>Mumbai</value>
			</set>
		</property>

		<property name="course">
			<map>
				<entry key="java" value="2 month" />
				<entry key="python" value="3 month" />
				<entry key="Spring" value="1.4 month" />
			</map>
		</property>
	</bean>



</beans>